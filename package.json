{
  "name": "portfolio-website",
  "description": "Yash Totale's Portfolio Website",
  "private": true,
  "license": "MIT",
  "homepage": "http://yashtotale.web.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/YashTotale/portfolio-v2/"
  },
  "bugs": {
    "url": "https://github.com/YashTotale/portfolio-v2/issues/"
  },
  "author": {
    "name": "Yash Totale",
    "email": "totaleyash@gmail.com",
    "url": "http://yashtotale.web.app/"
  },
  "scripts": {
    "make": "bash ./scripts/make.sh",
    "build": "react-scripts build --stats",
    "eject": "react-scripts eject",
    "start": "NODE_ENV=development react-scripts start",
    "test": "react-scripts test --passWithNoTests",
    "analyze": "bash ./scripts/analyze.sh",
    "deploy:local": "serve -s build",
    "deploy:private": "firebase deploy --only hosting:private",
    "deploy:public": "firebase deploy --only hosting:public",
    "deploy:firestore:rules": "firebase deploy --only firestore:rules",
    "deploy:storage:rules": "firebase deploy --only storage:rules",
    "data": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' ts-node scripts/data/index.ts",
    "books": "npm run books:get && npm run books:upload",
    "books:get": "sh scripts/books/get-books.sh",
    "books:upload": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' ts-node ./scripts/books/upload-books.ts",
    "postinstall": "husky install",
    "lint": "npx lint-check",
    "lint:staged": "lint-staged"
  },
  "dependencies": {
    "@contentful/rich-text-plain-text-renderer": "^14.1.2",
    "@contentful/rich-text-react-renderer": "^15.4.0",
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
