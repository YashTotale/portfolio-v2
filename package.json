    "firebase": "^8.3.3",
    "lodash.startcase": "^4.4.0",
    "lodash.throttle": "^4.1.1",
    "mime": "^2.5.2",
    "moment": "^2.29.1",
    "notistack": "^2.0.2",
    "preval.macro": "^5.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-firebaseui": "^5.0.2",
    "react-google-recaptcha": "^2.1.0",
