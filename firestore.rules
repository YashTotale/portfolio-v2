rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read: if false;
      allow write: if false;
    }

    match /contact/{docId} {
    	allow read: if false;
      allow write: if false;
      allow create: if true;
    }

    match /contact-errors/{docId} {
    	allow read: if false;
      allow write: if false;
      allow create: if true;
    }
  }
}
